[user]
	name = <%= config['git']['config']['name'] %>
	email = <%= config['git']['config']['email'] %>
[alias] 
	tree = log --graph --all --decorate --pretty=oneline --abbrev-commit
	featuremenge = merge --no-ff
[branch]
	autosetupmerge = true
[core]
	excludesfile = <%= config['symlinks_path'] %>/.gitignore
